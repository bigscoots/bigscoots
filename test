<VirtualHost 69.162.134.94:80>
  ServerName pathak-properties.com
  ServerAlias mail.pathak-properties.com www.pathak-properties.com
  DocumentRoot /home/pathakproperties/public_html
  ServerAdmin webmaster@pathak-properties.com
  UseCanonicalName Off
  CustomLog /etc/apache2/logs/domlogs/pathak-properties.com combined
  <IfModule log_config_module>
    <IfModule logio_module>
      CustomLog /etc/apache2/logs/domlogs/pathak-properties.com-bytes_log "%{%s}t %I .\n%{%s}t %O ."
    </IfModule>
  </IfModule>
  ## User pathakproperties # Needed for Cpanel::ApacheConf
  <IfModule userdir_module>
    <IfModule !mpm_itk.c>
      <IfModule !ruid2_module>
        <IfModule !mod_passenger.c>
          UserDir disabled
          UserDir enabled pathakproperties
        </IfModule>
      </IfModule>
    </IfModule>
  </IfModule>

  # Enable backwards compatible Server Side Include expression parser for Apache versions >= 2.4.
  # To selectively use the newer Apache 2.4 expression parser, disable SSILegacyExprParser in
  # the user's .htaccess file.  For more information, please read:
  #    http://httpd.apache.org/docs/2.4/mod/mod_include.html#ssilegacyexprparser
  <IfModule include_module>
    <Directory "/home/pathakproperties/public_html">
      SSILegacyExprParser On
    </Directory>
  </IfModule>



  <IfModule suphp_module>
    suPHP_UserGroup pathakproperties pathakproperties
  </IfModule>
  <IfModule suexec_module>
    <IfModule !mod_ruid2.c>
      SuexecUserGroup pathakproperties pathakproperties
    </IfModule>
  </IfModule>
  <IfModule ruid2_module>
    RMode config
    RUidGid pathakproperties pathakproperties
  </IfModule>
  <IfModule mpm_itk.c>
    # For more information on MPM ITK, please read:
    #   http://mpm-itk.sesse.net/
    AssignUserID pathakproperties pathakproperties
  </IfModule>
  <IfModule mod_passenger.c>
    PassengerUser pathakproperties
    PassengerGroup pathakproperties
  </IfModule>

  <IfModule alias_module>
    ScriptAlias /cgi-bin/ /home/pathakproperties/public_html/cgi-bin/
  </IfModule>


  # To customize this VirtualHost use an include file at the following location
  # Include "/etc/apache2/conf.d/userdata/std/2_4/pathakproperties/pathak-properties.com/*.conf"
</VirtualHost>
